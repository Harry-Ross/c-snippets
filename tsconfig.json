{
	"$schema": "https://json.schemastore.org/tsconfig.json",
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2024",
		"outDir": "out",
		"lib": ["ES2024"],
		"esModuleInterop": true,
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedParameters": true
	},
	"exclude": ["node_modules", ".vscode-test"]
}
